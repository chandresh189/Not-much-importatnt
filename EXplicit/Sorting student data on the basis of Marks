#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

int main()
{
	int roll[]={17, 20, 15, 1, 5};
	int marks[]={80, 75, 93, 78, 84};
	
	pair<int ,int> p[5];
	
	for(int i=0;i<5;i++)
	{
		p[i]={marks[i],roll[i]};
	}
	sort(p,p+5);
	for(int i=0;i<5;i++)
	{
		cout<<p[i].first<<" --> "<<p[i].second<<endl;
   }

return 0;
}
